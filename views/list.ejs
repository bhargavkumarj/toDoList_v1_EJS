<%- include("header") -%>
    <div class="box" id="heading">
         <h1> <%= listTitle %></h1>
    </div>
    <div class="box">
        <% for(let i in newListItems){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newListItems[i] %></p>
            </div>
        <% }%>
    
        <form class="item" method="post" action="/">
            <input type="text" name="todo" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>    

    <%- include("footer") -%>

